{
  "context_servers": {
    "mcp-server-figma": {
      "enabled": true,
      "settings": {
        "figma_api_key": "xxxx_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
      }
    }
  },
  "agent": {
    "always_allow_tool_actions": true,
    "default_model": {
      "provider": "openrouter",
      "model": "openrouter/free"
    },
    "default_profile": "write",
    "play_sound_when_agent_done": true,
    "use_modifier_to_send": true
  },
  "buffer_font_size": 18.0,
  "edit_predictions": {
    "provider": "supermaven",
    "disabled_globs": ["*-env.yaml"]
  },
  "icon_theme": "Colored Zed Icons Theme Dark",
  "project_panel": {
    "hide_gitignore": true
  },
  "soft_wrap": "editor_width",
  "theme": {
    "mode": "system",
    "light": "Dracula Light (Alucard)",
    "dark": "Dracula Solid"
  },
  "ui_font_size": 18.0,
  "vim_mode": true
}
