{
  "plugin": [
    "oh-my-opencode@3.4.0",
    "opencode-antigravity-auth@1.4.6",
    // "@plannotator/opencode@latest",
    // "@tarquinen/opencode-dcp@latest",
  ],
  "$schema": "https://opencode.ai/config.json",
  "autoupdate": true,
  "default_agent": "plan",
  "mcp": {
    "context7": {
      "headers": { "CONTEXT7_API_KEY": "{env:CONTEXT7_API_KEY}" },
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
    },
    "figma": {
      "type": "local",
      "command": [
        "bunx",
        "figma-developer-mcp",
        "--figma-api-key={env:FIGMA_API_KEY}",
        "--stdio",
      ],
    },
  },
  "model": "opencode/kimi-k2.5-free",
  "permission": {
    "bash": "ask",
    "doom_loop": "ask",
    "edit": "ask",
    "external_directory": "ask",
    "lsp": "ask",
    "skill": "ask",
    "task": "ask",
  },
  "provider": {
    "aihubmix": {
      "models": {
        "gemini-3-flash-preview-free": {
          "name": "Gemini 3 Flash Preview Free (AIHubMix)",
          "cost": { "input": 0, "output": 0 },
        },
      },
    },
    "google": {
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": { "thinkingLevel": "low" },
            "high": { "thinkingLevel": "high" },
          },
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "minimal": { "thinkingLevel": "minimal" },
            "low": { "thinkingLevel": "low" },
            "medium": { "thinkingLevel": "medium" },
            "high": { "thinkingLevel": "high" },
          },
        },
        "antigravity-claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5 (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
        "antigravity-claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } },
          },
        },
        "antigravity-claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } },
          },
        },
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
        "gemini-2.5-pro": {
          "name": "Gemini 2.5 Pro (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
        "gemini-3-flash-preview": {
          "name": "Gemini 3 Flash Preview (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
        "gemini-3-pro-preview": {
          "name": "Gemini 3 Pro Preview (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
      },
    },
    "openrouter": {
      "models": {
        "openrouter/auto": {
          "name": "Auto Router",
          "cost": { "input": 5, "output": 25 },
          "limit": { "context": 1000000, "output": 1000000 },
          "modalities": {
            "input": ["text", "image", "audio", "pdf", "video"],
            "output": ["text", "image"],
          },
        },
        "openrouter/free": {
          "name": "Free Models Router",
          "cost": { "input": 0, "output": 0 },
          "limit": { "context": 100000, "output": 100000 },
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"],
          },
          "reasoning": true,
        },
        "xiaomi/mimo-v2-flash": {
          "name": "Xiaomi: MiMo-V2-Flash",
          "cost": { "input": 0.1, "output": 0.3 },
          "limit": { "context": 262144, "output": 32000 },
          "modalities": { "input": ["text"], "output": ["text"] },
          "reasoning": true,
        },
      },
    },
  },
  "small_model": "opencode/gpt-5-nano",
}
