{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    // "oh-my-opencode@3.8.3",
    // "opencode-antigravity-auth@1.6.0",
    // "@plannotator/opencode@latest",
  ],
  "autoupdate": true,
  "default_agent": "plan",
  "mcp": {
    "context7": {
      "headers": { "CONTEXT7_API_KEY": "{env:CONTEXT7_API_KEY}" },
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
    },
    "framelink": {
      "type": "local",
      "command": ["bunx", "figma-developer-mcp", "--figma-api-key={env:FIGMA_API_KEY}", "--stdio"],
    },
    "shadcn": {
      "type": "local",
      "command": ["bunx", "shadcn@latest", "mcp"],
    },
  },
  "model": "opencode/kimi-k2.5-free",
  "permission": {
    "bash": "ask",
    "doom_loop": "ask",
    "edit": "ask",
    "external_directory": "ask",
    "lsp": "ask",
    "skill": "ask",
    "task": "ask",
  },
  "provider": {
    "aihubmix": {
      "models": {
        "coding-glm-5-free": {},
        "coding-minimax-m2.5-free": {},
        "gemini-3-flash-preview-free": {
          "name": "Gemini 3 Flash Preview Free (AIHubMix)",
          "cost": { "input": 0, "output": 0 },
          "limit": { "context": 1048576, "output": 65536 },
        },
        "kimi-for-coding-free": {},
        "mimo-v2-flash-free": {},
      },
      "whitelist": [],
    },
    "anthropic": {
      // "whitelist": ["claude-haiku-4-5", "claude-opus-4-6", "claude-sonnet-4-6"],
      "whitelist": [],
    },
    "github-copilot": {
      "whitelist": [
        "claude-haiku-4.5",
        "claude-opus-4.6",
        "claude-sonnet-4.6",
        "gemini-3-flash-preview",
        "gemini-3.1-pro-preview",
        "gpt-5-mini",
        "gpt-5.2",
        "gpt-5.2-codex",
        "grok-code-fast-1",
      ],
    },
    "github-models": {
      "whitelist": [],
    },
    "google": {
      "models": {
        "antigravity-gemini-3.1-pro": {
          "name": "Gemini 3.1 Pro (Antigravity)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"]
          },
          "variants": {
            "low": { "thinkingLevel": "low" },
            "high": { "thinkingLevel": "high" },
          },
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "minimal": { "thinkingLevel": "minimal" },
            "low": { "thinkingLevel": "low" },
            "medium": { "thinkingLevel": "medium" },
            "high": { "thinkingLevel": "high" },
          },
        },
        "antigravity-claude-sonnet-4-6-thinking": {
          "name": "Claude Sonnet 4.6 Thinking (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } },
          },
        },
        "antigravity-claude-opus-4-6-thinking": {
          "name": "Claude Opus 4.6 Thinking (Antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } },
          },
        },
        "gemini-3-flash-preview": {
          "name": "Gemini 3 Flash Preview (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
        "gemini-3-pro-preview": {
          "name": "Gemini 3 Pro Preview (Gemini CLI)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
      },
      "whitelist": [
        "antigravity-claude-opus-4-6-thinking",
        "antigravity-claude-sonnet-4-6-thinking",
        "antigravity-gemini-3-flash",
        "antigravity-gemini-3.1-pro",
        "gemini-3-flash-preview",
        "gemini-3-pro-preview",
      ],
    },
    "nvidia": {
      "whitelist": [
        "deepseek-ai/deepseek-v3.2",
        "minimaxai/minimax-m2.1",
        "moonshotai/kimi-k2-thinking",
        "moonshotai/kimi-k2.5",
        "openai/gpt-oss-120b",
        "z-ai/glm4.7",
        "z-ai/glm5",
      ],
    },
    "ollama-cloud": {
      "whitelist": [
        "gemini-3-flash-preview",
        "gemini-3-pro-preview",
        "glm-5",
        "kimi-k2.5",
        "minimax-m2.5",
      ],
    },
    "openrouter": {
      "models": {
        "openrouter/auto": {
          "name": "Auto Router",
          "cost": { "input": 5, "output": 25 },
          "limit": { "context": 1000000, "output": 1000000 },
          "modalities": {
            "input": ["text", "image", "audio", "pdf", "video"],
            "output": ["text", "image"],
          },
        },
        "openrouter/free": {
          "name": "Free Models Router",
          "cost": { "input": 0, "output": 0 },
          "limit": { "context": 100000, "output": 100000 },
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"],
          },
          "reasoning": true,
        },
        "xiaomi/mimo-v2-flash": {
          "name": "Xiaomi: MiMo-V2-Flash",
          "cost": { "input": 0.1, "output": 0.3 },
          "limit": { "context": 262144, "output": 32000 },
          "modalities": { "input": ["text"], "output": ["text"] },
          "reasoning": true,
        },
      },
      "whitelist": [
        "anthropic/claude-haiku-4.5",
        "anthropic/claude-opus-4.6",
        "anthropic/claude-sonnet-4.6",
        "arcee-ai/trinity-large-preview:free",
        "deepseek/deepseek-v3.2",
        "google/gemini-3-flash-preview",
        "google/gemini-3.1-pro-preview",
        "minimax/minimax-m2.5",
        "moonshotai/kimi-k2-thinking",
        "moonshotai/kimi-k2.5",
        "openai/gpt-5.2",
        "openai/gpt-5.2-codex",
        "openrouter/auto",
        "openrouter/free",
        "openrouter/sherlock-dash-alpha",
        "openrouter/sherlock-think-alpha",
        "stepfun/step-3.5-flash:free",
        "x-ai/grok-4",
        "x-ai/grok-4.1-fast",
        "x-ai/grok-code-fast-1",
        "xiaomi/mimo-v2-flash",
        "z-ai/glm-4.5v",
        "z-ai/glm-5",
      ],
    },
    "zai-coding-plan": {
      "whitelist": [
        "glm-4.5-air",
        "glm-4.6v",
        "glm-4.7",
        "glm-4.7-flash",
        "glm-5",
      ],
    },
  },
  "small_model": "opencode/gpt-5-nano",
}
